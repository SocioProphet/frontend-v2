{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server",
      "config": { "maxLambdaSize": "50mb" }
    }
  ],
  "env": {
    "NODE_ENV": "staging",
    "API_URL": "https://energidataservice.dk/api/3/action/",
    "WP_URL": "https://edscms.home.blog/",
    "THEME": "eds",
    "LOCALES": "en,da",
    "TRANSLATIONS": "/themes/eds/i18n"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
