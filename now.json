{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server",
      "env": {
        "state": "staging",
        "API_URL": "https://demo.ckan.org/api/3/action/",
        "WP_URL": "https://edscms.home.blog/"
      }
    },
    {
      "src": "index.js",
      "use": "@now/node-server",
      "env": {
        "state": "development",
        "API_URL": "http://127.0.0.1:5000/api/3/action/",
        "WP_URL": "http://127.0.0.1:6000"
      }
    },
    {
      "src": "index.js",
      "use": "@now/node-server",
      "env": {
        "state": "staging",
        "API_URL": "https://energidataservice.dk/api/3/action/",
        "WP_URL": "https://edscms.home.blog/"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
